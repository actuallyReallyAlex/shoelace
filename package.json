{
  "name": "shoelacejs",
  "version": "0.1.0",
  "description": "An application to convert files into JSON strings.",
  "main": "lib/main.js",
  "scripts": {
    "copy": "copyfiles -u 1 src/app.html lib",
    "clean": "rimraf lib",
    "build": "yarn clean && babel src --out-dir lib && yarn copy",
    "start": "electron ./lib/main.js"
  },
  "repository": "https://github.com/alexlee-dev/shoelace.git",
  "bugs": {
    "url": "https://github.com/alexlee-dev/shoelace/issues"
  },
  "homepage": "https://www.github.com/alexlee-dev/shoelace",
  "author": "alexlee-dev <alex@alexlee.dev>",
  "license": "MIT",
  "dependencies": {
    "electron": "^6.0.12",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "copyfiles": "^2.1.1",
    "rimraf": "^3.0.0"
  }
}
